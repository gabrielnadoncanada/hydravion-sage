    "@ravorona/eslint-config-typescript": "^2.0.0",
    "@ravorona/stylelint-config": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^4.22.0",
    "@typescript-eslint/parser": "^4.22.0",
