{
  "extends": [
    "plugin:vue/base",
    "plugin:vue/essential",
    "plugin:@typescript-eslint/recommended",
    "@vue/typescript/recommended",
    "@ravorona/typescript"
  ],
  "plugins": [
    "vue",
    "@typescript-eslint"
  ],
  "parser": "vue-eslint-parser",
  "parserOptions": {
    "parser": "@typescript-eslint/parser",
    "sourceType": "module",
    "allowImportExportEverywhere": false,
    "codeFrame": false,
    "ecmaVersion": 2020
  }
}
